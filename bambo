#!/bin/bash
bambo_dir=$( cd "$( dirname "${BASH_SOURCE[0]}" )" >/dev/null 2>&1 && pwd )
utils="$bambo_dir/utils.sh"

case $1 in
		d|git|help) # Script tags
			script=$1
			shift
		;;
		*) # Default script = d
			script=d
		;;
esac


function_name=$1; shift
script_dir="$bambo_dir/$script"

source $script_dir/${script}.sh

